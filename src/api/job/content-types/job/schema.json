{
  "kind": "collectionType",
  "collectionName": "jobs",
  "info": {
    "singularName": "job",
    "pluralName": "jobs",
    "displayName": "jobOffer",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "description": {
      "type": "text",
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "title": {
      "type": "string",
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "salary": {
      "type": "string",
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "category": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::category.category"
    },
    "country": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::country.country",
      "inversedBy": "job_offers"
    },
    "skills": {
      "type": "text",
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "comments": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::comment.comment",
      "mappedBy": "job_offer"
    },
    "likes": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::like.like",
      "mappedBy": "job_offer"
    },
    "applicants": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::applicant.applicant",
      "mappedBy": "job_offer"
    },
    "type": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "enumeration",
      "enum": [
        "Freelance",
        "contract"
      ]
    },
    "certificates": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::certificate.certificate",
      "inversedBy": "job_offers"
    },
    "publishDate": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "datetime",
      "required": true
    },
    "languages": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::language.language",
      "mappedBy": "job_offers"
    },
    "users_permissions_user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "job_offers"
    }
  }
}
