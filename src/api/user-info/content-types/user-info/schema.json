{
  "kind": "collectionType",
  "collectionName": "user_infos",
  "info": {
    "singularName": "user-info",
    "pluralName": "user-infos",
    "displayName": "userInfo",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "birthDate": {
      "type": "date",
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "sex": {
      "type": "enumeration",
      "enum": [
        "male",
        "female"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "nationalID": {
      "type": "string",
      "required": true,
      "unique": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "phoneNumber": {
      "type": "string",
      "unique": true,
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "residence_country": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::country.country"
    },
    "nationalities": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::nationality.nationality",
      "inversedBy": "user_infos"
    },
    "users_permissions_user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "user_infos"
    },
    "photo": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "comments": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::comment.comment",
      "mappedBy": "user_infos"
    },
    "applicants": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::applicant.applicant",
      "mappedBy": "user_info"
    },
    "certificate": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::certificate.certificate",
      "inversedBy": "user_infos"
    },
    "languages": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::language.language",
      "mappedBy": "user_infos"
    },
    "job": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::job-title.job-title",
      "inversedBy": "user_infos"
    },
    "cv": {
      "allowedTypes": [
        "files"
      ],
      "type": "media",
      "multiple": false,
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "required": false
    }
  }
}
