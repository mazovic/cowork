{
  "kind": "collectionType",
  "collectionName": "companies",
  "info": {
    "singularName": "company",
    "pluralName": "companies",
    "displayName": "company",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "name": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string",
      "required": true,
      "unique": true
    },
    "company_locateds": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::country.country",
      "inversedBy": "companies"
    },
    "companySize": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "enumeration",
      "enum": [
        "employees1-9 ",
        "employees10-49 ",
        "employees50-99 ",
        "more"
      ],
      "required": true
    },
    "categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::category.category",
      "inversedBy": "companies"
    },
    "users_permissions_user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "company"
    },
    "languages": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::language.language",
      "inversedBy": "companies"
    },
    "adminConfirmation": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "boolean",
      "default": false,
      "required": true
    },
    "companyLogo": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    }
  }
}
